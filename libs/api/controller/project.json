{
    "name": "controller",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/api/controller/src",
    "projectType": "library",
    "tags": [],
    "targets": {
        "build": {
            "executor": "@nx/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/libs/api/controller",
                "main": "libs/api/controller/src/index.ts",
                "tsConfig": "libs/api/controller/tsconfig.lib.json",
                "assets": ["libs/api/controller/*.md"]
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint"
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "libs/api/controller/jest.config.ts"
            }
        }
    }
}
